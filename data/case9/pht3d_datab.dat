10  mHy_carb = tot("Hy_carb")
20  mO2      = mol("O2")
30  mNO3     = tot("N(5)")
40  mSO4     = tot("S(6)")
50  mFE2     = tot("Fe(2)")
65  mFE3     = mFE3_max - mFE2
60  if (mHy_carb <= 1e-08) then goto 200
70  if (mFE3     <= 1e-08) then goto 200
90  inhib_ox     = 0.001/1000/32 / (0.001/1000/32 + mO2)
100 inhib_no3    = 0.001/1000/62 / (0.001/1000/62 + mNO3)
110 monod_fe3    = mFE3 / (0.5/1000/55.847 + mFE3)
120 rate         = parm(1) / 86400 * mHy_carb * inhib_ox
130 rate         = rate * inhib_no3 * monod_fe3
140 rate         = rate * 36.0
#180 moles = rate * m * (m/m0) * time
180 moles = rate * time
#190 if (moles > m) then moles = m
200 SAVE moles
-end

