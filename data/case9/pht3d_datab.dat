RATES

##########
#Oxygen
#Y_O2/HC = (9 * 32) / 92 = 3.14 (mg/mg) (using mol. weight of toluene)
#C7H8 + 9O2 + 3H2O = 7HCO3- + 7H+  -> Y_O2/HC = 9 (mol/mol)
##########
O(0)
-start
10  mHy_carb = tot("Hy_carb")
20  mO2 =      mol("O2")
30  if (mHy_carb <= 1e-08) then goto 200
40  if (mO2      <= 1e-08) then goto 200
90  monod_ox     = mO2 / (0.5/1000/32 + mO2)
120 rate         = parm(1) * 86400 * mHy_carb * monod_ox
130 rate         = rate * 9.0
#180 moles = rate * m * (m/m0) * time
180 moles = rate * time
#190 if (moles > m) then moles = m
200 SAVE moles
-end

##########
#Nitrate
#Y_NO3/HC = (7.2 * 62) / 92 = 4.90 (mg/mg) (using mol. weight of toluene)
#C7H8 + 7.2 NO3- + 0.2H+ = 7HCO3- + 3.6 N2 + 0.6H2O  -> Y_NO3/HC = 7.2 (mol/mol)
##########
N(5)
-start
10  mHy_carb = tot("Hy_carb")
20  mO2      = mol("O2")
30  mNO3     = tot("N(5)")
40  if (mHy_carb <= 1e-08) then goto 200
50  if (mNO3     <= 1e-08) then goto 200
90  inhib_ox     = 0.001/1000/32 / (0.001/1000/32 + mO2)
100 monod_no3    = mNO3 / (0.5/1000/62 + mNO3)
120 rate         = parm(1) / 86400 * mHy_carb * inhib_ox
130 rate         = rate * monod_no3
140 rate         = rate * 7.2
#180 moles = rate * m * (m/m0) * time
180 moles = rate * time
#190 if (moles > m) then moles = m
200 SAVE moles
-end

##########
#Fe(2)
#Y_Fe/HC = (36*55.847) / 92 = 21.80 (mg/mg) (using mol. weight of toluene)
#C7H8 + 36Fe+3 + 21H2O = 7HCO3- + 36Fe+2 + 43H+  -> Y_Fe/HC = 36 (mol/mol)
##########
#
Fe(2)
-start
